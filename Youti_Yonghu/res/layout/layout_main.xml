<com.youti.view.SlidingMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_menu"
    xmlns:zhy="http://schemas.android.com/apk/res/com.example.youti_geren"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:background="@drawable/bk_bg"
    android:scrollbars="none"
    zhy:rightPadding="110dp"
    tools:context="com.youti.yonghu.activity.MainActivity" >

    
     <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

       <com.youti.view.LeftMenuView 
           
            android:layout_width="match_parent"
            android:layout_height="match_parent"
           ></com.youti.view.LeftMenuView>

        <include layout="@layout/main_home" />
        <com.youti.view.RightMenuView 
              android:layout_width="match_parent"
            android:layout_height="match_parent"
            ></com.youti.view.RightMenuView>
         <!-- <include layout="@layout/main_menu_right" /> -->
        
    </LinearLayout>

</com.youti.view.SlidingMenu>
